package Main;

public class MentalMath {

	
	public int splitNJoin(int num1) {
		if (num1 <10) {
		return num1;
		}
		else {
		int num2 = num1 / 10;
		int num3 = num1 % 10;
		return num2 + num3;
		}
	}
	
	public int splitNJoin2(int num1) {
		int num2 = num1 / 10;
		int num3 = num1 % 10;
		return num2 + num3;
	}
}
