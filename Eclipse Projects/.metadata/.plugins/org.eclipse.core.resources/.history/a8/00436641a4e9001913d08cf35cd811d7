package Main;

public class Iteration {

	public void iteration1() {
		
		for(int a = 100; a <= 200; a++) {
			System.out.println("A");
		}
		
	}
	
	public void iteration2() {
		
		for(int a = 100; a <= 200; a++) {
			if(a % 2 == 0){
				System.out.println("-");
			}
			else {
				System.out.println("*");
			}
		}
		
		
	}
	
	public void iteration3(){
		
		for(int i = 0; i < 10; i++) {
			for(int j = 0; j < 10; j++) {
				System.out.println(j + 1);
			}
			
		}
	}
	
	public void iteration4(int num1){
		
		for(int i = 0; i < num1; i++) {
			for(int j = 0; j < 10; j++) {
				System.out.println(j + 1);
			}
			
		}
	}
	
	public int iteration5() {
		for(int i = 1; i <= 100; i++) {
			if (i == 100) {
				String number = "One hundred";
			}
			else {
				if (i < 10) {
					if (i == 1){
						String number = "one";
					}
					else if (i == 2) {
						String number = "two";
					}
					else if (i == 3) {
						String number = "three";
					}
					else if (i == 4) {
						String number = "four";
					}
					else if (i == 5) {
						String number = "five";
					}
					else if (i == 6) {
						String number = "six";
					}
					else if (i == 7) {
						String number = "seven";
					}
					else if (i == 8) {
						String number = "eight";
					}
					else {
						String number = "nine";
					}
					}
					else {
					
						int num2 = i / 100;
						int ex1 = i % 100;
						int num3 = ex1 / 10;
						int num4 = ex1 % 10;		
				
				
					}
				}
				return number;
			}
		
		
		
	}
	
}
